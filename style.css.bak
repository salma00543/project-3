*
{
	margin:0;
	padding:0;
}
a
{
	text-decoration:none;
}
.container
{
	padding:0;
    width:1150px;
}
.logo
{
	background:#EDEDED;
	display:block;
	overflow:hidden;
	padding:20px 30px;
	font-size:40px;
	
}
.logo h1
{
	color:#192026;
}
.menu
{
	padding:0 auto;
}
.menu ul
{
	background:#13252F;
}
.menu ul li
{
	list-style:none;
	display:inline-block;
	padding:10px 30px;
}
.menu ul li a
{
	text-decoration:none;
	color:white;
}
.menu ul li:hover
{
	background:white;
}
.menu ul li a:hover
{
	color:black;
}
